    <link rel="stylesheet" href="css/style.css">
    <title>Room Booking Dashboard</title>
</head>
<body>
    <div class="menu">
        <div class="buttons">
            <button class="button classrooms">Classrooms</button>
            <button class="button spaces">Spaces</button>
            <button class="button meetingrooms">Meeting Rooms</button>
            <button class="button other">Others</button>
        </div>
        <div class="select">
            <label for="select_room">Select Room: </label>
            <select id="select_room"></select>
        </div>
        <div class="select">
            <label for="select_fixed">Select fixed: </label>
            <select id="select_fixed">
                <option value="none">None</option>
                <option value="fixed">Fixed</option>
                <option value="not fixed">Not Fixed</option>
            </select>
        </div>
        <div class="select">
            <label for="select_recording">Select recording: </label>
            <select id="select_recording">
                <option value="none">None</option>
                <option value="recording">Recording</option>
                <option value="isnt Recording">Isn't Recording</option>
            </select>
        </div>
    </div>

    <div id="calendarContainer">
        <div id="calendar"></div>
    </div>
    
</body>
<script src="/js/index.js"></script>
